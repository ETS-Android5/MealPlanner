<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mealplanner"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="25"
        android:targetSdkVersion="29" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true"
        android:extractNativeLibs="false"
        android:icon="@drawable/applogo"
        android:label="EatUP"
        android:roundIcon="@drawable/applogo"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.example.mealplanner.RecipeInDetail" />
        <activity android:name="com.example.mealplanner.RecipeList" />
        <activity android:name="com.example.mealplanner.SplashScreen" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.mealplanner.ForgetPassword" />
        <activity android:name="com.example.mealplanner.Usersmain" />
        <activity android:name="com.example.mealplanner.SignUp" />
        <activity android:name="com.example.mealplanner.Login" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <provider
            android:name="com.squareup.picasso.PicassoProvider"
            android:authorities="com.example.mealplanner.com.squareup.picasso"
            android:exported="false" />
    </application>

</manifest>